{
  "project": {
    "src_path": "src",
    "build_path": "dist",
    "root_file_name": "index.html"
  },
  "links_replacements": [
    {
      "target": "\"modules.html\"",
      "replace_by": "\"/modules\""
    },
    {
      "target": "\"index.html\"",
      "replace_by": "\"/\""
    },
    {
      "target": "\"privacy-policy.html\"",
      "replace_by": "\"/privacy-policy\""
    },
    {
      "target": "\"terms-and-conditions.html\"",
      "replace_by": "\"/terms-and-conditions\""
    }
  ],
  "file_links_replacement": [
    {
      "target": "\"css/",
      "replace_by": "\"../css/"
    },
    {
      "target": "images/",
      "replace_by": "../images/"
    },
    {
      "target": "\"js/",
      "replace_by": "\"../js/"
    }
  ],
  "transforms": [
    {
      "name": "modules",
      "filepath": "modules.html",
      "move_to_subfolder": true,
      "apply_links_replacements": true,
      "apply_file_links_replacements": true
    },
    {
      "name": "index",
      "filepath": "index.html",
      "move_to_subfolder": false,
      "apply_links_replacements": true,
      "apply_file_links_replacements": false
    },
    {
      "name": "privacy-policy",
      "filepath": "privacy-policy.html",
      "move_to_subfolder": true,
      "apply_links_replacements": true,
      "apply_file_links_replacements": true
    },
    {
      "name": "terms-and-conditions",
      "filepath": "terms-and-conditions.html",
      "move_to_subfolder": true,
      "apply_links_replacements": true,
      "apply_file_links_replacements": true
    }
  ]
}
